{
  "name": "telegrambotboilerplate",
  "version": "1.0.0",
  "description": "telegram bot boiler plate",
  "main": "index.js",
  "scripts": {
    "https-fwd": "node https-forwarding.js",
    "build:dev": "webpack --mode development --config ./webpack/backend.config.js && webpack --mode development --config ./webpack/frontend.dev.config.js",
    "build:prod": "webpack --mode production --config ./webpack/backend.config.js && webpack --mode production --config ./webpack/frontend.prod.config.js",
    "dev:server": "set MODE=develop&&webpack ",
    "build": "webpack --mode development",
    "start": "node ./dist/server.js",
    "dev:tg": "concurrently \"npm run build:dev\" \"npm run start\""
  },
  "author": "crocodeev",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.12",
    "@mui/styled-engine-sc": "^5.10.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "materialize-css": "^1.0.0",
    "ngrok": "^4.3.3",
    "node-telegram-bot-api": "^0.59.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.0.1",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.5.0",
    "css-loader": "^6.7.1",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-middleware": "^5.3.3",
    "webpack-dev-server": "^4.11.1",
    "webpack-hot-middleware": "^2.25.2",
    "webpack-node-externals": "^3.0.0"
  }
}
